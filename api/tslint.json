{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exculde": [
            "node_modules"
        ]
    },
    "jsRules": {},
    "rules": {
        "no-console": true,
        "quotemark": [
            true,
            "single"
        ],
        "no-trailing-whitespace": [
            true,
            "ignore-comments",
            "ignore-jsdoc",
            "ignore-template-strings",
            "ignore-blank-lines"
        ],
        "trailing-comma": [
            false,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "ordered-imports": false,
        "max-line-length": [
            true,
            140
        ],
        "object-literal-sort-keys": false,
        "variable-name": {
            "options": [
                "allow-leading-underscore"
            ]
        },
        "no-string-literal": false,
        "one-variable-per-declaration": false,
        "no-var-requires": true,
        "no-empty": false
    },
    "rulesDirectory": []
}