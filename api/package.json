{
  "name": "api",
  "version": "1.0.0",
  "description": "Services for Grubber",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkcwebdev/Grubber.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tkcwebdev/Grubber/issues"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run lint && nyc mocha",
    "lint": "./node_modules/.bin/tslint -p tsconfig.json",
    "start": "npm run build && node ./dist/index.js"
  },
  "dependencies": {
    "@types/node": "12.12.2",
    "@types/nconf": "0.0.37",
    "@types/mysql": "2.15.4",
    "body-parser": "1.19.0",
    "cookie": "0.4.1",
    "cookie-parser": "1.4.5",
    "crypto-js": "4.0.0",
    "csrf": "3.1.0",
    "express": "4.17.1",
    "log4js": "6.3.0",
    "memory-cache": "0.2.0",
    "mysql": "2.18.1",
    "nconf": "0.10.0",
    "request": "2.88.2",
    "typescript": "3.4.3",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.3",
    "@types/mocha": "8.0.3",
    "@types/body-parser": "1.17.0",
    "@types/cookie": "0.3.1",
    "@types/cookie-parser": "1.4.1",
    "@types/crypto-js": "3.1.39",
    "@types/express": "4.11.1",
    "@types/memory-cache": "0.2.0",
    "@types/sinon": "4.3.1",
    "@types/supertest": "2.0.4",
    "@types/uuid": "3.4.3",
    "chai": "4.2.0",
    "core-js": "3.6.5",
    "mocha": "8.1.3",
    "nyc": "15.0.1",
    "sinon": "9.0.2",
    "supertest": "4.0.2",
    "ts-node": "8.10.1",
    "tslint": "6.1.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "statements": 95,
    "branches": 95,
    "functions": 95,
    "lines": 95,
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "middlewares.ts",
      "logger.ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".ts"
    ],
    "cache": true,
    "all": true,
    "temp-directory": "./coverage-tmp",
    "report-dir": "./coverage"
  },
  "mocha": {
    "spec": [
      "test/**/*.spec.ts"
    ],
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "fullTrace": true,
    "bail": true,
    "exit": true,
    "timeout": 10000
  },
  "homepage": "https://github.com/tkcwebdev/Grubber#readme"
}
